package springMVC.NLoveB.service.iter;

import java.util.List;

import springMVC.NLoveB.po.Links;

public interface SerLinksIter {
	/**
	 * 前台显示可见的友情链接
	 * @return
	 */
	List<Links> allLinks();

	/**
	 * 后台显示所有的链接
	 * @return
	 */
	String allAdminLinks();
	
	/**
	 * 更新或新增链接
	 * @param datajsons
	 * @return
	 */
	String saveupLinks(String datajsons);
	
	/**
	 * 删除友情链接
	 * @param linkids
	 * @return
	 */
	String delLinks(String linkids);
}
